{
    "name": "kms/froala-editor-bundle",
    "type": "symfony-bundle",
    "description": "Provides a Froala editor integration for Symfony 3.",
    "keywords": [ "froala", "editor", "wysiwyg" ],
    "license": "MIT",
    "version": "2.1.0",
    "authors": [
        {
            "name": "Simon Guionniere",
            "email": "simon.guionniere@gmail.com"
        },
        {
            "name": "Stefan Neculai",
            "email": "stefan.neculai@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": "2.*",
        "symfony/form": ">=2.1",
        "twig/twig": ">=1.1"
    },
    "require-dev": {
        "symfony/twig-bundle": "2.*",
        "symfony/yaml": "~2.0"
    },
    "autoload": {
        "psr-4": { "KMS\\FroalaEditorBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.*-dev"
        }
    }
}
